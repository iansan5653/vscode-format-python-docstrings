{
	"name": "format-python-docstrings",
	"displayName": "Format Python Docstrings",
	"description": "Format docstrings in Python using PEP 257 standards.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^10.14.1",
        "@types/diff": "^4.0.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"child_process": "^1.0.2",
		"diff": "^4.0.1",
		"util": "^0.11.1"
	}
}
